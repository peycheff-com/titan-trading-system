{
  "files": [],
  "references": [
    { "path": "./packages/shared" },
    { "path": "./services/titan-ai-quant" },
    { "path": "./packages/titan-backtesting" },
    { "path": "./services/titan-brain" },
    { "path": "./services/titan-console-api" },
    { "path": "./apps/titan-console" },
