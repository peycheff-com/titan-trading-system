{
  "name": "titan-phase3-sentinel",
  "version": "1.0.0",
  "description": "Titan Phase 3 - The Sentinel: Market-neutral hedge fund system for basis arbitrage, funding rate collection, and vacuum arbitrage",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:all": "npm run build",
    "start": "npm run build && node dist/index.js",
    "start:headless": "npm run build && node dist/index.js --headless",
    "dev": "tsc && node --watch dist/index.js",
    "test": "jest --config jest.config.cjs",
    "test:watch": "jest --config jest.config.cjs --watch",
    "test:coverage": "jest --config jest.config.cjs --coverage",
    "test:unit": "jest --config jest.config.cjs --selectProjects unit",
    "test:integration": "jest --config jest.config.cjs --selectProjects integration",
    "test:property": "jest --config jest.config.cjs --selectProjects property",
    "lint": "npm run lint:check",
    "lint:check": "eslint src/**/*.ts src/**/*.tsx",
    "lint:fix": "eslint src/**/*.ts src/**/*.tsx --fix",
    "format:check": "prettier --check 'src/**/*.{ts,tsx}'",
    "format:write": "prettier --write 'src/**/*.{ts,tsx}'"
  },
  "keywords": [
    "trading",
    "algorithmic",
    "crypto",
    "basis-arbitrage",
    "market-neutral",
    "hedge-fund",
    "funding-rate",
    "delta-neutral",
    "polymarket"
  ],
  "author": "Titan Trading System",
  "license": "MIT",
  "dependencies": {
    "@titan/shared": "*",
    "ink": "^4.4.1",
    "nats": "^2.29.3",
    "react": "^18.2.0",
    "undici": "^7.18.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "eslint-plugin-functional": "^9.0.2",
    "fast-check": "^4.4.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
