    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.0",
    "@types/node-schedule": "^2.1.5",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-functional": "^9.0.2",
    "eslint-plugin-prettier": "^5.5.5",
    "fast-check": "^4.4.0",
    "jest": "^29.7.0",
