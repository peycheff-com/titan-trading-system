{
  "name": "titan-phase2-hunter",
  "version": "1.0.0",
  "description": "Titan Phase 2 - The Hunter: Holographic Market Structure Engine for institutional-grade swing trading",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:all": "npm run build",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --selectProjects unit",
    "test:integration": "jest --selectProjects integration",
    "test:property": "jest --selectProjects property",
    "test:console": "jest --selectProjects console",
    "test:ci": "jest --ci --coverage --watchAll=false --maxWorkers=2",
    "lint": "npm run lint:check",
    "lint:check": "eslint src/**/*.ts src/**/*.tsx",
    "lint:fix": "eslint src/**/*.ts src/**/*.tsx --fix",
    "format:check": "prettier --check 'src/**/*.{ts,tsx}'",
    "format:write": "prettier --write 'src/**/*.{ts,tsx}'"
  },
  "keywords": [
    "trading",
    "algorithmic",
    "crypto",
    "holographic",
    "market-structure",
    "institutional",
    "swing-trading"
  ],
  "author": "Titan Trading System",
  "license": "MIT",
  "dependencies": {
    "@titan/shared": "*",
    "@types/node-fetch": "2.6.4",
    "dotenv": "^16.6.1",
    "fast-check": "^4.4.0",
    "node-fetch": "2.6.7",
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.27",
    "@types/react": "^18.2.0",
    "@types/ws": "^8.5.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-extended": "^4.0.2",
    "jest-junit": "^16.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
