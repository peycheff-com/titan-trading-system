"use strict";
/**
 * Test imports to debug the issue
 */
describe('Import Test', () => {
    it('should import HologramScanner', () => {
        const { HologramScanner } = require('../../src/engine/HologramScanner');
        expect(HologramScanner).toBeDefined();
        expect(typeof HologramScanner).toBe('function');
    });
    it('should import EventEmitter', () => {
        const { EventEmitter } = require('events');
        expect(EventEmitter).toBeDefined();
        expect(typeof EventEmitter).toBe('function');
        const emitter = new EventEmitter();
        expect(emitter).toBeDefined();
    });
    it('should create a class that extends EventEmitter', () => {
        const { EventEmitter } = require('events');
        class TestClass extends EventEmitter {
            constructor() {
                super();
            }
        }
        const instance = new TestClass();
        expect(instance).toBeDefined();
        expect(instance).toBeInstanceOf(EventEmitter);
    });
});
