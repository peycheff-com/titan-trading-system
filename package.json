{
  "name": "titan-trading-system",
  "private": true,
  "description": "Titan Trading System Monorepo",
  "workspaces": [
    "services/*",
    "packages/*",
    "apps/*"
  ],
  "packageManager": "npm@11.6.2",
  "scripts": {
    "start": "echo 'Use npm run start:<service> to start a specific service'",
    "start:brain": "npm start -w services/titan-brain",
    "start:execution": "cd services/titan-execution-rs && cargo run --release",
    "start:console": "npm start -w apps/titan-console",
    "start:scavenger": "npm start -w services/titan-phase1-scavenger",
    "start:hunter": "npm start -w services/titan-phase2-hunter",
    "start:sentinel": "npm start -w services/titan-phase3-sentinel",
    "start:powerlaw-lab": "npm start -w services/titan-powerlaw-lab",
    "start:ai-quant": "npm start -w services/titan-ai-quant",
    "start:prod": "bash scripts/ops/start_production.sh",
    "build": "npx turbo run build && npm run sota:provenance",
    "test": "npx turbo run test",
    "test:all": "npx turbo run test",
    "lint:all": "npx turbo run lint",
    "lint:fix": "npx turbo run lint:fix",
    "lint:docs": "markdownlint 'docs/**/*.md' && find docs -name '*.md' -print0 | xargs -0 -n1 markdown-link-check -q",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "validate:config": "npx ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/validate-configs.ts",
    "sota:circular": "bash scripts/sota/check_circular.sh",
    "sota:arch": "bash scripts/sota/check_architecture.sh",
    "sota:complexity": "npx ts-node scripts/sota/rank_complexity.ts",
    "sota:god": "python3 scripts/sota/god_class_scan.py",
    "sota:dead": "bash scripts/sota/check_dead_code.sh",
    "sota:zombie": "python3 scripts/sota/zombie_scan.py",
    "sota:secrets": "python3 scripts/sota/secret_scan.py",
    "sota:flake": "bash scripts/sota/check_flakiness.sh",
    "sota:immutability": "npx eslint -c scripts/sota/immutability.config.mjs \"services/*/src/**/*.{ts,tsx}\" \"packages/*/src/**/*.{ts,tsx}\"",
    "sota:audit": "npm audit --audit-level=high",
    "sota:license": "bash scripts/sota/check_license.sh",
    "sota:mutation": "npx stryker run stryker.config.mjs",
    "sota:bundle": "npx ts-node scripts/sota/check_bundle_size.ts",
    "sota:profile": "echo 'Install 0x manually: npx 0x -o -- npm start:brain' # Interactive profiling",
    "sota:impact": "bash scripts/sota/check_impact.sh",
    "sota:correctness": "npx jest tests/correctness",
    "sota:typecheck": "tsc -b",
    "sota:deps": "depcheck && npm audit --audit-level=high",
    "sota:provenance": "npx ts-node scripts/security/generate_provenance.ts",
    "sota:sbom": "syft . -o cyclonedx-json=.generated/sbom/sbom.json",
    "sota:valuation": "bash scripts/valuation/generate_valuation.sh",
    "sota:secrets:deep": "python3 scripts/sota/secret_scan_deep.py",
    "sota:api": "npx ts-node scripts/sota/check_api_drift.ts",
    "sota:rust:fmt": "cd services/titan-execution-rs && cargo fmt -- --check",
    "sota:rust:clippy": "cd services/titan-execution-rs && cargo clippy -- -D warnings",
    "sota:rust:test": "cd services/titan-execution-rs && cargo test",
    "sota:rust:coverage": "./scripts/valuation/generate_rust_coverage.sh",
    "sota:perf": "vitest run --config apps/titan-console/vitest.config.ts apps/titan-console/src/test/perf/",
    "sota:db": "npx ts-node scripts/sota/check_db_migrations.ts",
    "sota:unit": "npm test",
    "sota:integration": "echo 'Run: npm run test:integration'",
    "sota:e2e": "npx playwright test --config apps/titan-console/e2e/playwright.config.ts",
    "sota:docs:links": "npx ts-node --esm scripts/docs/validate_links.ts",
    "sota:docs:truth": "npx ts-node --esm scripts/docs/check_truth.ts",
    "sota:docs:all": "npm run sota:docs:truth && npm run sota:docs:links",
    "sota:edge:validation": "npx ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/sota/check_edge_validation.ts",
    "sota:contracts:schemas": "npx ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/sota/check_contracts_schemas.ts",
    "sota:replay:determinism": "npx ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/sota/verify_determinism.ts",
    "sota:health:deps": "npx ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/sota/check_health_deps.ts",
    "sota:runbooks": "npx ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/sota/check_runbooks.ts",
    "sota:migrations:safety": "npx ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/sota/check_migrations.ts",
    "sota:metrics:required": "npx ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/sota/check_metrics_probes.ts",
    "docs:serve": "mkdocs serve",
    "docs:build": "mkdocs build",
    "sota:all": "npm run sota:circular && npm run sota:arch && npm run sota:complexity && npm run sota:god && npm run sota:dead && npm run sota:zombie && npm run sota:secrets && npm run sota:immutability && npm run sota:audit && npm run sota:license && npm run sota:bundle && npm run sota:correctness && npm run sota:typecheck && npm run sota:deps && npm run sota:rust:fmt && npm run sota:rust:clippy && npm run sota:rust:test && npm run sota:perf && npm run sota:db && npm run sota:unit && npm run sota:docs:all && npm run sota:edge:validation && npm run sota:contracts:schemas && npm run sota:replay:determinism && npm run sota:health:deps && npm run sota:runbooks && npm run sota:migrations:safety && npm run sota:metrics:required"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "nats": "^2.29.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@stryker-mutator/core": "^9.4.0",
    "@stryker-mutator/jest-runner": "^9.4.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.0",
    "ajv": "^8.17.1",
    "commander": "^14.0.2",
    "depcheck": "^1.4.7",
    "dependency-cruiser": "^17.3.7",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-functional": "^9.0.2",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-sonarjs": "^3.0.5",
    "glob": "^10.4.5",
    "jest": "^30.2.0",
    "js-yaml": "^4.1.0",
    "knip": "^5.82.1",
    "license-checker": "^25.0.1",
    "madge": "^8.0.0",
    "markdown-link-check": "^3.14.2",
    "markdownlint-cli": "^0.47.0",
    "prettier": "^3.8.0",
    "redis": "^4.7.0",
    "tinybench": "^2.9.0",
    "ts-jest": "^29.4.6",
    "turbo": "^2.3.3",
    "typescript-eslint": "^8.53.0",
    "undici": "^7.19.1",
    "ws": "^8.18.0",
    "zod": "^4.3.6"
  },
  "overrides": {
    "lodash": ">=4.17.23",
    "d3-color": "^3.1.0",
    "prismjs": ">=1.30.0",
    "lodash-es": ">=4.17.22",
    "@modelcontextprotocol/sdk": "1.26.0",
    "@isaacs/brace-expansion": "5.0.1",
    "refractor": {
      "prismjs": ">=1.30.0"
    },
    "@chevrotain/cst-dts-gen": {
      "lodash-es": ">=4.17.22"
    },
    "@chevrotain/gast": {
      "lodash-es": ">=4.17.22"
    },
    "chevrotain": {
      "lodash-es": ">=4.17.22"
    }
  }
}
